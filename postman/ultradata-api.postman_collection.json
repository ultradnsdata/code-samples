		{
			"name": "get auth token",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"ultradata-auth and ultradata-refresh must exist\", function () {",
							"pm.expect(pm.environment.has(\"ultradata-auth\")).to.equal(true);",
							"pm.expect(pm.environment.has(\"ultradata-refresh\")).to.equal(true);",
							"var jsonData = pm.response.json();",
							"pm.environment.set(\"ultradata-auth\", jsonData.access_token);",
							"pm.environment.set(\"ultradata-refresh\", jsonData.refresh_token);",
							"});",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "urlencoded",
					"urlencoded": [
						{
							"key": "username",
							"value": "{{ultradata-user}}",
							"type": "text"
						},
						{
							"key": "password",
							"value": "{{ultradata-pw}}",
							"type": "text"
						},
						{
							"key": "grant_type",
							"value": "password",
							"type": "text"
						}
					]
				},
				"url": {
					"raw": "{{ultradata-api}}/v1/authorization/token",
					"host": [
						"{{ultradata-api}}"
					],
					"path": [
						"v1",
						"authorization",
						"token"
					]
				},
				"description": "how to get an authorization token."
			},
			"response": []
		},
